{
  "name": "egg-starter",
  "version": "1.0.0",
  "description": "这是一个可以让您在短时间内构建Api的框架，它是基于EggJs官方的simple模板进行二次封装开发",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.6.4",
    "@qc2168/swagger-egg": "^1.7.6",
    "ajv": "^8.17.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.12",
    "egg": "^3.27.1",
    "egg-scripts": "2",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "file-system-cache": "^2.4.7",
    "fs-extra": "^11.2.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.0",
    "node-jsencrypt": "^1.0.0",
    "random-string": "^0.2.0",
    "sequelize": "^6.37.4",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "egg-bin": "6",
    "egg-mock": "5",
    "eslint": "^9.8.0",
    "eslint-config-egg": "13",
    "globals": "^15.9.0",
    "sequelize-cli": "^6.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-egg-api",
    "stop": "egg-scripts stop --title=egg-server-egg-api",
    "dev": "dotenvx run -f .env.development -- egg-bin dev",
    "test": "npm run lint -- --fix && npm run test:local",
    "test:local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QC2168/egg-starter"
  },
  "author": "qc2168",
  "license": "MIT"
}
